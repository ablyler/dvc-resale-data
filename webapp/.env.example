# DVC Resale Data Environment Configuration

# API Configuration - REQUIRED
# Replace 'your-function-app' with your actual Azure Function App name
# Example: https://rofr-scraper-func.azurewebsites.net/api
REACT_APP_API_BASE_URL=https://your-function-app.azurewebsites.net/api

# For local development, you can use:
# REACT_APP_API_BASE_URL=http://localhost:7071/api

# Environment mode
REACT_APP_ENV=production

# Optional: Analytics and tracking
REACT_APP_ENABLE_ANALYTICS=false

# Optional: Feature flags
REACT_APP_ENABLE_ADVANCED_FEATURES=true
REACT_APP_ENABLE_EXPORT_FEATURES=true

# Build configuration
GENERATE_SOURCEMAP=false

# Note: All API endpoints are configured for anonymous access
# No authentication tokens or API keys are required
# The Azure Functions have been configured with CORS enabled
